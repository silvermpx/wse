{
  "name": "wse-client",
  "version": "1.0.3",
  "description": "WSE (WebSocket Engine) React client. Type-safe hooks, auto-reconnect, offline queue, E2E encryption.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "keywords": [
    "websocket",
    "react",
    "hooks",
    "real-time",
    "events",
    "pubsub",
    "encryption",
    "rust",
    "zustand"
  ],
  "author": "silvermpx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/silvermpx/wse.git",
    "directory": "client"
  },
  "homepage": "https://github.com/silvermpx/wse",
  "bugs": {
    "url": "https://github.com/silvermpx/wse/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "publishConfig": {
    "provenance": true
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0",
    "pako": "^2.1.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "zustand": ">=4"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/pako": "^2.0.4",
    "@types/react": "^18.3.0",
    "react": "^18.3.1",
    "typescript": ">=5",
    "vitest": "^2.0.0",
    "zustand": "^4.5.0"
  }
}
